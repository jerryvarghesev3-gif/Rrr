ll



private val currentDevice = MedDevices.CEN   // CEN app
// private val currentDevice = MedDevices.DEN // DEN app

init {
    if (currentDevice == MedDevices.CEN) {
        load()
    } else {
        showWrongProductPopup()
    }
}





private fun showWrongProductPopup() {
    updateState { state ->
        state.copy(
            incompatibleProductDialogVisible = true,
            incompatibleProductMessage = "Wrong product. Please use the correct application."
        )
    }
}
